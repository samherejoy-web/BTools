# Production Environment Configuration for MarketMind AI Frontend
# Copy this file to .env for production deployment

# Backend API URL - Update with your production domain
REACT_APP_BACKEND_URL=https://yourdomain.com

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false
BUILD_PATH=build

# SEO Configuration
REACT_APP_SITE_URL=https://yourdomain.com
REACT_APP_SITE_NAME=MarketMind AI
REACT_APP_SITE_DESCRIPTION=Discover and compare the best B2B tools with AI-powered insights

# Analytics & Tracking (Add your IDs)
REACT_APP_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
REACT_APP_GOOGLE_TAG_MANAGER_ID=GTM-XXXXXXX

# Social Media
REACT_APP_TWITTER_HANDLE=@yourdomain
REACT_APP_LINKEDIN_URL=https://linkedin.com/company/yourdomain

# Feature Flags
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_SERVICE_WORKER=true

# Performance
REACT_APP_ENABLE_LAZY_LOADING=true
REACT_APP_ENABLE_CODE_SPLITTING=true

# Security
REACT_APP_ENABLE_CSP=true
REACT_APP_ENABLE_HSTS=true